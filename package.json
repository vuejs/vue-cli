{
  "name": "vue-cli",
  "version": "1.5.0",
  "description": "A simple CLI for scaffolding Vue.js projects.",
  "preferGlobal": true,
  "bin": {
    "vue": "bin/vue"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/vue-cli.git"
  },
  "keywords": [
    "vue",
    "cli",
    "spa"
  ],
  "author": "Evan You",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vuejs/vue-cli/issues"
  },
  "homepage": "https://github.com/vuejs/vue-cli#readme",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint && npm run e2e",
    "lint": "eslint test/test.js lib bin/* --env mocha",
    "e2e": "rimraf test/e2e/mock-template-build/*.* && BABEL_ENV=development mocha test/e2e/test.js --slow 1000 --compilers js:babel-core/register"
  },
  "dependencies": {
    "async": "^2.0.0-rc.2",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "consolidate": "^0.14.0",
    "download-git-repo": "^0.1.1",
    "handlebars": "^4.0.5",
    "inquirer": "^0.12.0",
    "metalsmith": "^2.1.0",
    "minimatch": "^3.0.0",
    "ora": "^0.2.1",
    "read-metadata": "^1.0.0",
    "request": "^2.67.0",
    "rimraf": "^2.5.0",
    "semver": "^5.1.0",
    "uid": "0.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.7.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.4.5"
  }
}
