{
  "name": "vue-cli",
  "version": "2.7.0",
  "description": "A simple CLI for scaffolding Vue.js projects.",
  "preferGlobal": true,
  "bin": {
    "vue": "bin/vue",
    "vue-init": "bin/vue-init",
    "vue-list": "bin/vue-list",
    "vue-build": "bin/vue-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/vue-cli.git"
  },
  "keywords": [
    "vue",
    "cli",
    "spa"
  ],
  "author": "Evan You",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vuejs/vue-cli/issues"
  },
  "homepage": "https://github.com/vuejs/vue-cli#readme",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint && npm run e2e",
    "lint": "eslint test/e2e/test.js lib bin/* --env mocha",
    "e2e": "rimraf test/e2e/mock-template-build/*.* && cross-env BABEL_ENV=development mocha test/e2e/test.js --slow 1000 --compilers js:babel-core/register"
  },
  "dependencies": {
    "async": "^2.0.0-rc.2",
    "buble-loader": "^0.4.0",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "consolidate": "^0.14.0",
    "css-loader": "^0.26.1",
    "download-git-repo": "^0.2.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "friendly-errors-webpack-plugin": "^1.1.2",
    "handlebars": "^4.0.5",
    "html-webpack-plugin": "^2.26.0",
    "inquirer": "^0.12.0",
    "metalsmith": "^2.1.0",
    "minimatch": "^3.0.0",
    "multimatch": "^2.1.0",
    "ora": "^0.2.1",
    "post-compile-webpack-plugin": "^0.1.0",
    "read-metadata": "^1.0.0",
    "request": "^2.67.0",
    "rimraf": "^2.5.0",
    "semver": "^5.1.0",
    "uid": "0.0.2",
    "user-home": "^2.0.0",
    "validate-npm-package-name": "^2.2.2",
    "vue": "2.1.8",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "2.1.8",
    "webpack": "2.2.0-rc.3",
    "webpack-dev-server": "2.2.0-rc.0",
    "webpack-merge": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "eslint": "^2.7.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.4.5"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
